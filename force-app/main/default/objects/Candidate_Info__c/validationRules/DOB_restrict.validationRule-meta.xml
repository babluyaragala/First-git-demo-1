<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DOB_restrict</fullName>
    <active>true</active>
    <errorConditionFormula>Date_of_birth__c  &lt;=   DATE(2006,12,05)</errorConditionFormula>
